<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.husky.busi.dao.StudentMapper">
    <select id="selectStudentById" resultType="com.husky.entity.Student">
        select *
        from student
        where s_id = #{id}
    </select>
    <select id="selectStudentByPage" resultType="com.husky.entity.Student">
        select *
        from student
    </select>
</mapper>